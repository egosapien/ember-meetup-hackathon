{{#md-card title='Organizations'}}
  {{#md-card-content}}
    {{#md-collection content=model as |org|}}

      {{#link-to "orgs.org.repos" org.id class="collection-item avatar"}}
        <img src={{org.iconUrl}} alt={{org.name}} class="circle" />
        {{org.name}}
      {{/link-to}}
    {{/md-collection}}
  {{/md-card-content}}
{{/md-card}}
